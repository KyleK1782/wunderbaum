{
  "name": "wunderbaum",
  "version": "0.0.1-0",
  "title": "A tree control.",
  "description": "JavaScript tree control.",
  "main": "dist/wunderbaum.umd.js",
  "types": "dist/wunderbaum.d.ts",
  "module": "dist/perrsisto.esm.min.js",
  "homepage": "https://github.com/mar10/wunderbaum",
  "author": {
    "name": "Martin Wendt",
    "url": "http://careers.stackoverflow.com/martin-wendt"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mar10/wunderbaum"
  },
  "bugs": {
    "url": "https://github.com/mar10/wunderbaum/issues"
  },
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://raw.githubusercontent.com/mar10/wunderbaum/master/LICENSE.txt"
    }
  ],
  "exports": {
    ".": {
      "require": "./dist/wunderbaum.umd.min.js",
      "import": "./dist/wunderbaum.esm.min.js"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "keywords": [
    "ajax",
    "cache",
    "localStorage",
    "sessionStorage",
    "persistence"
  ],
  "devDependencies": {
    "@rollup/plugin-typescript": "^6.1.0",
    "@types/jest": "^26.0.15",
    "concurrently": "^6.0.0",
    "eslint": "^7.8.1",
    "eslint-config-jquery": "^3.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "grunt": "^1.1.0",
    "grunt-contrib-connect": "^2.0.0",
    "grunt-contrib-qunit": "^4.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-exec": "^3.0.0",
    "grunt-yabs": "^1.3.0",
    "http-server": "^0.12.3",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "qunit": "^2.14.0",
    "rollup": "^2.32.1",
    "rollup-plugin-modify": "^3.0.0",
    "rollup-plugin-scss": "^2.6.1",
    "terser": "^5.3.8",
    "ts-jest": "^26.4.3",
    "ts-node": "^9.1.1",
    "tslib": "^2.0.3",
    "typedoc": "^0.20.26",
    "typescript": "^4.1.5"
  },
  "nodemonConfig": {
    "watch": [
      "src/"
    ],
    "ext": "ts,scss",
    "exec": "npm run build:js -s",
    "ignore": [
      "node_modules/",
      ".git",
      "test/*",
      "docs/*"
    ],
    "delay": "2500"
  },
  "scripts": {
    "test": "grunt ci --verbose",
    "docs": "typedoc && touch docs/.nojekyll",
    "build:types": "tsc -t esnext --moduleResolution node -d --emitDeclarationOnly --outFile dist/wunderbaum.d.ts src/wunderbaum.ts",
    "build:js": "rollup -c rollup.config.js",
    "build:minjs:umd": "terser dist/wunderbaum.umd.js --compress --mangle --source-map \"url='wunderbaum.umd.min.js.map'\" --output dist/wunderbaum.umd.min.js",
    "build:minjs:esm": "terser dist/wunderbaum.esm.js --compress --mangle --source-map \"url='wunderbaum.esm.min.js.map'\" --output dist/wunderbaum.esm.min.js",
    "build:minjs": "npm run build:minjs:esm -s && npm run build:minjs:umd -s",
    "build": "npm run build:js -s && npm run build:minjs -s && npm run build:types -s && npm run docs",
    "watch:umd": "nodemon --watch src --ext 'ts' -x \"npm run build:minjs:umd\"",
    "watch": "nodemon",
    "serve": "http-server test -p 8080 -o /",
    "dev": "concurrently \"http-server . -p 8080 -o /test \" \"nodemon\""
  },
  "npmName": "wunderbaum",
  "npmFileMap": [
    {
      "basePath": "dist",
      "files": [
        "wunderbaum.esm.js",
        "wunderbaum.esm.min.js",
        "wunderbaum.esm.min.js.map",
        "wunderbaum.umd.js",
        "wunderbaum.umd.min.js",
        "wunderbaum.umd.min.js.map"
      ]
    }
  ]
}
